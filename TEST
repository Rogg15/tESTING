			end
			end
			game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
				if key == "e" then
		Pause()
				end
				end)

			_G.savedhumanoidpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			end
		end
		})
		


