					View.MouseButton1Click:Connect(function()
						local Target = Player(Target.Text)
						if not Target then
							game.StarterGui:SetCore("SendNotification", {
								Title = "Notification";
								Text = "Player Left! or Either User not Found";
								Duration = 5;
							})
							return
						end
						_G.spectating = true
					while _G.spectating == true do
						game.Workspace.CurrentCamera.CameraSubject = Target.Character.Humanoid
						wait()
						end
					end)

					Unview.MouseButton1Click:Connect(function()
						_G.spectating = false
						game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
					end)


					local Pause = function()
						Loop:Disconnect()
						local vectorZero = Vector3.new(0, 0, 0)
						player.Character.PrimaryPart.Velocity = vectorZero
						player.Character.PrimaryPart.RotVelocity = vectorZero
						playerr.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
					local vectorZero = Vector3.new(0, 0, 0)
					player.Character.PrimaryPart.Velocity = vectorZero
					player.Character.PrimaryPart.RotVelocity = vectorZero
					player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
					end;


					Fling.MouseButton1Click:Connect(function()
						if Fling.Text == 'Fling' then
							Fling.Text = 'Unfling'
						local PUSSY = Player(Target.Text)
						local player = game.Players.LocalPlayer


							local Loop
							local OldFlingPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
							local loopFunction = function()

								local success,err = pcall(function()
									local FlingEnemy = PUSSY.Character
									if FlingEnemy and player.Character then
										FlingTorso = FlingEnemy.HumanoidRootPart
										local dis = 3.85
										local increase = 6
										if FlingEnemy.Humanoid.MoveDirection.X < 0 then
											xchange = -increase
										elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
											xchange = increase
										else
											xchange = 0
										end
										if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
											zchange = -increase
										elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
											zchange = increase
										else
											zchange = 0
										end          
										if player.Character then
											player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
											player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
											player.Character.HumanoidRootPart.Velocity = Vector3.new(50000,500000,50000)
										end
									end
								end)
								if err then
									print('fling error : ' .. err)
								end
							end;
							local Start = function()    
								OldFlingPos = player.Character.HumanoidRootPart.Position
								Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
							end;
							local Pause = function()
								Loop:Disconnect()
								local vectorZero = Vector3.new(0, 0, 0)
								player.Character.PrimaryPart.Velocity = vectorZero
								player.Character.PrimaryPart.RotVelocity = vectorZero
								player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
							local vectorZero = Vector3.new(0, 0, 0)
							player.Character.PrimaryPart.Velocity = vectorZero
							player.Character.PrimaryPart.RotVelocity = vectorZero
							player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
						end;
							Start()	
							repeat wait() until Fling.Text == 'Fling'
							Pause()
							local vectorZero = Vector3.new(0, 0, 0)
							player.Character.PrimaryPart.Velocity = vectorZero
							player.Character.PrimaryPart.RotVelocity = vectorZero
							player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
							local vectorZero = Vector3.new(0, 0, 0)
							player.Character.PrimaryPart.Velocity = vectorZero
							player.Character.PrimaryPart.RotVelocity = vectorZero
							player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
						else
							Fling.Text = 'Fling'
						end

					end)

					Flingv2.MouseButton1Click:Connect(function()
						if Flingv2.Text == 'Flingv2' then
							Flingv2.Text = 'Unflingv2'

						local PUSSY = Player(Target.Text)
						local player = game.Players.LocalPlayer


							local Loop
							local OldFlingPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
							local loopFunction = function()

								local success,err = pcall(function()
									local FlingEnemy = PUSSY.Character
									if FlingEnemy and player.Character then
										FlingTorso = FlingEnemy.HumanoidRootPart
										local dis = 3.85
										local increase = 6
										if FlingEnemy.Humanoid.MoveDirection.X < 0 then
											xchange = -increase
										elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
											xchange = increase
										else
											xchange = 0
										end
										if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
											zchange = -increase
										elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
											zchange = increase
										else
											zchange = 0
										end          
										if player.Character then
											player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
											player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
											player.Character.HumanoidRootPart.Velocity = Vector3.new(50000,500000,50000)
											game:GetService("Players").LocalPlayer.Character.Humanoid:UnequipTools()
											local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid:Clone()
											Humanoid.BreakJointsOnDeath = false
											game.Players.LocalPlayer.Character.Animate.Disabled = true
											game:GetService("Players").LocalPlayer.Character.Humanoid:Destroy()
											Humanoid.Parent = game:GetService("Players").LocalPlayer.Character
									
											game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
													for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
														if v:IsA("Tool") and v.Name == "potion" then
														v.Parent = game.Players.LocalPlayer.Character
														end
														end
										end
									end
									
								end)
								if err then
									print('fling error : ' .. err)
								end
							end;
							local Start = function()    
								OldFlingPos = player.Character.HumanoidRootPart.Position
								Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
							end;
							local Pause = function()
								Loop:Disconnect()
								local vectorZero = Vector3.new(0, 0, 0)
								player.Character.PrimaryPart.Velocity = vectorZero
								player.Character.PrimaryPart.RotVelocity = vectorZero
								player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
							local vectorZero = Vector3.new(0, 0, 0)
							player.Character.PrimaryPart.Velocity = vectorZero
							player.Character.PrimaryPart.RotVelocity = vectorZero
							player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
						end;
							Start()	
							repeat wait() until Flingv2.Text == 'Flingv2'
							Pause()
							local vectorZero = Vector3.new(0, 0, 0)
							player.Character.PrimaryPart.Velocity = vectorZero
							player.Character.PrimaryPart.RotVelocity = vectorZero
							player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
