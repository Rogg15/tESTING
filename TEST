					LoopGive.Parent = Frame
					LoopGive.BackgroundColor3 = Color3.fromRGB(36, 36, 38)
					LoopGive.BorderSizePixel = 0
					LoopGive.Position = UDim2.new(0.528260946, 0, 0.770866275, 0)
					LoopGive.Size = UDim2.new(0, 68, 0, 49)
					LoopGive.Font = Enum.Font.TitilliumWeb
					LoopGive.Text = "LoopGive"
