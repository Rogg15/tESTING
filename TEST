					end)

					Freeze.MouseButton1Click:Connect(function(a)

						local some = Player(Target.Text)
						if not Target then
							game.StarterGui:SetCore("SendNotification", {
								Title = "Notification";
								Text = "Player Left! or Either User not Found";
								Duration = 5;
							})
							return
						end

							some.Character:FindFirstChild("HumanoidRootPart").Anchored = true

						

					end)

					Unfreeze.MouseButton1Click:Connect(function(a)

						local some = Player(Target.Text)
						some.Character:FindFirstChild("HumanoidRootPart").Anchored = false

					end)
		end
		})
